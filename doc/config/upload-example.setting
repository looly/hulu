#上传文件的最大大小限制，-1表示无限制，单位byte，默认102400
file.size.max = 102400
#文件保存在内存的临界大小，当上传的文件小于这个数时保存于内存中，否则保存于硬盘的临时目录，默认8192
memory.threshold = 8192
#临时文件存放路径，默认使用系统临时文件目录
tmp.upload.path = 
#上传文件的扩展名限制，以逗号分隔，默认无限制
file.exts = 
#上传文件的扩展名是允许有还是不允许有（即白名单和黑名单），默认白名单
file.exts.allow = true